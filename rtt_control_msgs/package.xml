<package>
  <name>rtt_control_msgs</name>
  <version>2.6.0</version>
  <description>

    Provides an rtt typekit for ROS control_msgs messages.

    It allows you to use ROS messages transparently in
    RTT components and applications.

    This package was automatically generated by the
    create_rtt_msgs generator and should not be manually
    modified.

    See the http://ros.org/wiki/control_msgs documentation
    for the documentation of the ROS messages in this
    typekit.

  </description>
  <maintainer email="orocos-dev@lists.mech.kuleuven.be">Orocos Developers</maintainer>
  <author>create_rtt_msgs</author>
  <license>BSD</license>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>control_msgs</build_depend>
  <build_depend>rtt_rostopic</build_depend>

  <run_depend>control_msgs</run_depend>
  <run_depend>rtt_rostopic</run_depend>

  <build_depend>rtt_std_msgs</build_depend>
  <run_depend>rtt_std_msgs</run_depend>  <build_depend>rtt_geometry_msgs</build_depend>
  <run_depend>rtt_geometry_msgs</run_depend>  <build_depend>rtt_trajectory_msgs</build_depend>
  <run_depend>rtt_trajectory_msgs</run_depend>  <build_depend>rtt_actionlib_msgs</build_depend>
  <run_depend>rtt_actionlib_msgs</run_depend>

  <export>
    <rtt_plugin_depend>rtt_rostopic</rtt_plugin_depend>
    <rtt_plugin_depend>rtt_std_msgs</rtt_plugin_depend>    <rtt_plugin_depend>rtt_geometry_msgs</rtt_plugin_depend>    <rtt_plugin_depend>rtt_trajectory_msgs</rtt_plugin_depend>    <rtt_plugin_depend>rtt_actionlib_msgs</rtt_plugin_depend>
  </export>

</package>


